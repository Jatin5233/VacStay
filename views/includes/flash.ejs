<% if(success &&success.length){%>
    <div class="row">
        <div class="alert alert-success col-6 offset-2 alert-dismissible fade show flash" role="alert">
            <%=success%>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
          </div>
    </div>
    
<%}%>
<% if(error &&error.length){%>
    <div class="row">
        <div class="alert alert-danger col-6 offset-2 alert-dismissible fade show flash" role="alert">
            <%=error%>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
          </div>
    </div>
    
<%}%>
